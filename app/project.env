# Database configuration for PostgreSQL (running in container called "database-server-dab-p2-9e75e129-6c87-47c2-b29d-06bf837f3c7a")
POSTGRES_USER=username
POSTGRES_PASSWORD=password
POSTGRES_DB=database

# Database configuration for Flyway (used for database migrations)
FLYWAY_USER=username
FLYWAY_PASSWORD=password
FLYWAY_URL=jdbc:postgresql://database-server-dab-p2-9e75e129-6c87-47c2-b29d-06bf837f3c7a:5432/database

# Database configuration for PostgreSQL driver
PGUSER=username
PGPASSWORD=password
PGHOST=database-server-dab-p2-9e75e129-6c87-47c2-b29d-06bf837f3c7a
PGPORT=5432
PGDATABASE=database

# Deno cache location (avoid reloading depedencies over and over)
DENO_DIR=/app-cache